@import "../global"
@import "../reset"

body
  background-color: dark-red

.wrapper
  width: 66%;
  min-width: 320px;
  margin: 0 auto;
  height: 100%;
  background-color: white;

.app
  float: left;
  width: 100%;

.left-pad
  float: left;
  width: 75px;
  height: 100%;
  background: url('../img/filmside.png') repeat-y;

.right-pad
  float: right;
  width: 75px;
  height: 100%;
  background: url('../img/filmside.png') repeat-y;

.clear-fix
  clear: both;