h2(data-bind="text: title")
iframe.video-player(data-bind="attr: {src: youtubeUrl}" frameborder="0" allowfullscreen)

.button-area
  button.dislike(data-bind="click: $root.onLeft")
    include ../../public/img/dislike.svg
  button.like(data-bind="click: $root.onRight")
    include ../../public/img/like.svg